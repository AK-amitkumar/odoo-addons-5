                <div class="overlay">
                  <h2>Title2<br/></h2>
                  <a class="btn btn-default info" href="/shop" role="button">Show More</a>
                </div>
              </div>
            </div>
            <div class="col-xs-12 col-md-4">
              <div class="hover ehover1">
                <img class="img img-responsive" src="/theme_mj_zoom_tri/static/src/img/temp-1.jpg"/>
                <div class="overlay">
                  <h2>Title3<br/></h2>
                  <a class="info btn btn-default" href="/shop"  role="button">Show More</a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
  </template>
  <template id="place_into_bar" inherit_id="website.snippets" name="Place into bar">
  <xpath expr="//div[@id='snippet_structure']/div[@class='o_panel_body']" position="inside">
    <t t-snippet="theme_mj_zoom_tri.snippet_mj_zoom_tri"
       t-thumbnail="/theme_mj_zoom_tri/static/src/img/block_portfolio.png"/>
  </xpath>
</template>
</odoo>